{
    "name": "control-stock",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "ts-node src/app.ts",
        "dev": "ts-node-dev src/app.ts",
        "build": "tsc",
        "prod": "node dist/app.js",
        "typeorm": "typeorm-ts-node-esm",
        "migration:generate": "npm run typeorm -- migration:generate ./src/migrations/%npm_config_name% -d src/data-source.ts",
        "migration:run": "npm run typeorm -- migration:run -d src/data-source.ts",
        "start-db": "docker-compose up -d",
        "reset-db": "npm run docker:stop && npm run docker:remove && npm run docker:build && npm run migration:run",
        "docker:stop": "docker ps -q --filter \"ancestor=postgres:14.5\" | xargs -r docker stop",
        "docker:remove": "docker ps -a -q --filter \"ancestor=postgres:14.5\" | xargs -r docker rm",
        "docker:build": "docker-compose up -d",
        "test": "jest",
        "test:watch": "jest --watch"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@types/bcryptjs": "^2.4.6",
        "@types/express": "^4.17.21",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/supertest": "^6.0.2",
        "bcryptjs": "^2.4.3",
        "express": "^4.18.2",
        "jsonwebtoken": "^9.0.2",
        "mysql": "^2.18.1",
        "pg": "^8.11.3",
        "reflect-metadata": "^0.1.13",
        "typeorm": "0.3.19",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@types/jest": "^29.5.12",
        "@types/node": "^16.11.10",
        "jest": "^29.7.0",
        "supertest": "^6.3.4",
        "ts-jest": "^29.1.2",
        "ts-node": "10.9.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "4.5.2"
    }
}
